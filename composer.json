        },
        {
            "type": "package",
            "package": {
                "name": "deaemth/easychart",
                "version": "3.1.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/daemth/easychart/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
