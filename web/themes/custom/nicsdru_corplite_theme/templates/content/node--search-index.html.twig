{#
/**
 * @file
 * Theme override to display a node in search result view mode.
#}
<a class="card card--plain no-ext-icon" href="{{ url }}">
  <h3 class="card__title">{{ label }}</h3>
  {%- if content.field_summary %}
  <div class="card__summary">
    {{ content.field_summary }}
  </div>
  {% endif %}
  <div class="card__meta">
    {%- if content.field_published_date %}
      <span class="card__meta-item field-published-date">{{ 'Published'|t }} {{ content.field_published_date }}</span>
    {% endif -%}
    {%- if content.field_publication_type %}
      <span class="card__meta-item field-publication-type">{{ content.field_publication_type }}</span>
    {% endif -%}
  </div>
</a>
