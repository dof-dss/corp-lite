<?php

/**
 * @file
 * Contains nisra_taxonomy_navigator.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function nisra_taxonomy_navigator_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the nisra_taxonomy_navigator module.
    case 'help.page.nisra_taxonomy_navigator':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Manage your large taxonomies') . '</p>';
      return $output;

    default:
  }
}
